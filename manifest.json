{
  "name": "BookmarkManager",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A chrome extension developed to get rid of the ugly chrome bookmarks bar",
  "homepage_url": "https://github.com/NotMegalion/BookmarkManager",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/popup.html"
  },
  "permissions": [
    "bookmarks"
    ]
}
